datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id       Int     @id @default(autoincrement())
  username String
  Drank    Drank[]
}

model Drank {
  id     Int      @id @default(autoincrement())
  userId Int
  day    String
  time   DateTime
  Litre  Int[]

  user User @relation(fields: [userId], references: [id])
}
